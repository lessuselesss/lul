{
  "name": "Leader",
  "keys": {
    "\\": "nix fmt && nh os switch && nh clean all",
    "c": "nh clean all",
    "s": "nh os switch",
    "S": "sudo nixos-rebuild --flake . switch",
    "f": "nix fmt",
    "F": {
      "name": "flake",
      "keys": {
        "u": "nix flake update",
        "s": "nix flake show",
        "c": "nix flake check",
        "r": "nix repl"
      }
    }
  }
}
